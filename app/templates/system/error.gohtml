{{define "error"}}
    <section>
        <article class="page-title">
            <h1>{{.ErrorStatus}}</h1>
        </article>
        <article>
            <h2>{{.ErrorMessage}}</h2>
            <p>
                Go back
                <button class="btn btn-icon" onclick="window.history.back()">
                    <svg class="feather">
                        <use xlink:href="{{.Routes.BaseUrl}}/static/icons/feather-sprite.svg#arrow-left"/>
                    </svg>
                </button>
            </p>
        </article>
    </section>
{{end}}
